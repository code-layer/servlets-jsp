<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>product-app</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>ProductPage</display-name>
    <servlet-name>ProductPage</servlet-name>
    <servlet-class>in.datalayer.pages.ProductPage</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ProductPage</servlet-name>
    <url-pattern>/ProductPage</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ContactPage</display-name>
    <servlet-name>ContactPage</servlet-name>
    <servlet-class>in.datalayer.pages.ContactPage</servlet-class>
        <init-param>
      <description></description>
      <param-name>mobileNo</param-name>
      <param-value>91 - 98459 44444</param-value>
    </init-param>
    <init-param>
      <description></description>
      <param-name>emailId</param-name>
      <param-value>help@example.com</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>ContactPage</servlet-name>
    <url-pattern>/ContactPage</url-pattern>
  </servlet-mapping>
</web-app>